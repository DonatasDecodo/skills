{
  "name": "openclaw-smart-router",
  "version": "1.0.0",
  "description": "Intelligent model selection for OpenClaw agents - automatically route to optimal models based on complexity, budget, and learned patterns",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "setup": "node src/setup.js",
    "dashboard": "node src/dashboard.js",
    "test": "node --test"
  },
  "keywords": [
    "openclaw",
    "model-routing",
    "smart-router",
    "cost-optimization",
    "x402",
    "agent-economy",
    "ai",
    "pattern-learning"
  ],
  "author": "AtlasPA",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "express": "^4.18.2",
    "commander": "^12.0.0"
  },
  "openclaw": {
    "skill": true,
    "hooks": {
      "request:before": "hooks/request-before.js",
      "provider:after": "hooks/provider-after.js",
      "session:end": "hooks/session-end.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AtlasPA/openclaw-smart-router.git"
  },
  "homepage": "https://github.com/AtlasPA/openclaw-smart-router#readme"
}
