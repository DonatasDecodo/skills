{
  "agent_id": "liquidbot",
  "name": "LiquidBot",
  "version": "1.0.0",
  "description": "Professional liquidity pool manager for DEXes",
  "enabled": true,
  "triggers": {
    "keywords": [
      "add liquidity",
      "remove liquidity",
      "create pool",
      "lp management",
      "liquidbot",
      "pool position",
      "liquidity position",
      "uniswap lp",
      "provide liquidity",
      "withdraw liquidity",
      "lp tokens",
      "concentrated liquidity"
    ],
    "commands": ["/liquidbot", "/addlp", "/removelp", "/lpstatus"],
    "regex_patterns": [
      "add.*liquidity",
      "remove.*liquidity",
      "lp.*position",
      "pool.*management"
    ]
  },
  "pricing": {
    "payment_wallet": "0x4A9583c6B09154bD88dEE64F5249df0C5EC99Cf9",
    "accepted_tokens": ["ETH"],
    "plans": {
      "trial": {
        "duration_days": 2,
        "price_eth": 0.013,
        "display": "LiquidBot: 0.013 ETH (2-day) | 5 ETH (lifetime)",
        "features": ["basic_lp", "single_chain", "v2_only"]
      },
      "lifetime": {
        "duration_days": -1,
        "price_eth": 5.0,
        "display": "LiquidBot: 0.013 ETH (2-day) | 5 ETH (lifetime)",
        "features": ["all_protocols", "all_chains", "v3_concentrated", "auto_rebalance", "il_tracking"]
      }
    }
  },
  "chains": {
    "ethereum": {
      "chain_id": 1,
      "enabled": true,
      "rpc_env": "ETH_RPC_URL",
      "protocols": ["uniswap_v2", "uniswap_v3", "sushiswap"]
    },
    "base": {
      "chain_id": 8453,
      "enabled": true,
      "rpc_env": "BASE_RPC_URL",
      "protocols": ["uniswap_v3", "aerodrome"]
    },
    "polygon": {
      "chain_id": 137,
      "enabled": true,
      "rpc_env": "POLYGON_RPC_URL",
      "protocols": ["uniswap_v3", "quickswap", "sushiswap"]
    },
    "arbitrum": {
      "chain_id": 42161,
      "enabled": true,
      "rpc_env": "ARBITRUM_RPC_URL",
      "protocols": ["uniswap_v3", "sushiswap", "camelot"]
    }
  },
  "protocols": {
    "uniswap_v2": {
      "router": "0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D",
      "factory": "0x5C69bEe701ef814a2B6a3EDD4B1652CB9cc5aA6f"
    },
    "uniswap_v3": {
      "router": "0xE592427A0AEce92De3Edee1F18E0157C05861564",
      "factory": "0x1F98431c8aD98523631AE4a59f267346ea31F984",
      "position_manager": "0xC36442b4a4522E871399CD717aBDD847Ab11FE88"
    }
  },
  "automation": {
    "auto_approve": true,
    "slippage_default": 0.5,
    "slippage_max": 5.0,
    "gas_estimation": true,
    "simulation_before_tx": true
  },
  "limits": {
    "min_position_usd": 10,
    "max_slippage_percent": 10,
    "max_positions_per_user": 50
  }
}
